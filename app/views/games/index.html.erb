
<div style="width:100%; text-align: center;">
	<% @games.each do |game| %>
		<a href="#" onclick="go(<%=game.id%>);">
			<div>
				<img src="<%=game.image_url%>" style="width:100px; display: block; margin-left: auto; margin-right: auto;">
			</div>
		</a>
	<% end %>
</div>

<script>
	function go(game_id){
		window.location = "<%=URL%>" + "kakao_accounts?game_id=" + game_id; 
	}
</script>
